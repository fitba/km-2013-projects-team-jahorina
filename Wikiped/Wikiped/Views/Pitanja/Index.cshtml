@model IEnumerable<Wikiped.DBBL.DAL.Pitanja>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/Pocetna.cshtml";
}
<div class="wrapperleft">
@foreach (var item in ViewBag.AllPitanja)
{
    <div class="question-summary narrow">
    <div class="leftBox">
        <div class="votes">
             <div class="mini-counts">@item.BrojGlasova</div>
             <div>glas</div>
            </div>
        <div class="status unanswered">
            <div class="mini-counts">0</div>
             <div>odgovor</div>
        </div>
        <div class="views">
            <div class="mini-counts">@item.BrojPregleda</div>
            <div>pregled</div>
        </div>
    </div>
    <div class="summary">
        <h3> <a href="/pitanja/details/@item.PitanjeID">@item.Naziv</a></h3>
        <div class="tags t-html t-css t-google-chrome t-heroku t-zurb-foundation">
           
            @foreach (var tag in @ViewBag.GetTags(@item.PitanjeID))
            {
                 <a href="#" class="post-tag" title="" rel="tag">@tag</a> 
            }
            
        </div>
        <div class="started">
            <a href="/questions/14567110/website-content-shown-on-moz-but-not-chrome-with-heroku" class="started-link"><span title="2013-01-28 16:57:21Z" class="relativetime">1m ago</span></a>
            <a href="/users/1509401/jason328">jason328</a> <span class="reputation-score" title="reputation score" dir="ltr">634</span>
        </div>
    </div>
    </div>
}
